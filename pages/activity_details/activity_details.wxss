  height: 30rpx;
  margin-left: 20rpx;
}
.on{
  margin-top: 80rpx;
  position: relative;
}
.award_box{
  padding: 40rpx 30rpx;
  box-sizing: border-box;
}
.on .award_box .award_top{
  position: absolute;
  top: -35rpx;
}
.award_conent{
  display: flex;
  margin-bottom: 20rpx;
  font-size: 30rpx
}
.award_conent>view:nth-child(2){
  margin-left: 20rpx;
  display: flex;
  justify-content: flex-start;
}
.award_box>view:nth-child(2)>view, .award_box>view:nth-child(3)>view, .award_box>view:nth-child(4)>view, .award_box>view:nth-child(5)>view{
