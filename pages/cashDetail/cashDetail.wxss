/* pages/cashDetail/cashDetail.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.cashDetail{
  padding-top: 22rpx;
}
.cashDetail_box{
  width: 100%;
  height: 160rpx;
  padding: 0 22rpx;
  background-color: white;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  border-bottom: 1rpx solid #d3d1d1;
}
.cashDetail_top, .cashDetail_middle, .cashDetail_botton{
  /* padding-top: 26rpx; */
}
.cashDetail_top>text:nth-child(1){
 font-size: 32rpx;
 /* font-weight: 700; */
}
.cashDetail_top>text:nth-child(2){
  padding: 2rpx 8rpx;
  border-radius: 10rpx;
  background-color: #e6f4e6;
  margin-left: 20rpx;
  font-size: 24rpx;
  color: #76c179;
}
.cashDetail_middle>view:nth-child(2){
  font-size: 28rpx;
  color: #8f8f8f;
  text-align: right;
}
.cashDetail_middle>view:nth-child(1){
  font-size: 28rpx;
  color: red;
}
.cashDetail_botton>text:nth-child(1), .cashDetail_botton>text:nth-child(2){
  font-size: 28rpx;
  color: #8f8f8f;
}

/* <!-- 《无数据状态 --> */
.teamAward_all{
  width: 100%;
  height: 90%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.teamAward_imgt{
  margin-top: 300rpx;
}
.teamAward_imgt>image{
  width: 380rpx;
  height: 300rpx;
}
.teamAward_imgt>view{
  margin: 60rpx 60rpx;
  font-size: 30rpx;
}
