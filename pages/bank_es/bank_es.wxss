/* pages/bank_es/bank_es.wxss */
page{
  background: #f5f5f5;
}
.bank_box{
  padding-top:22rpx;
}
.bank{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 40rpx;
  background: white;
}
.bank>image{
  width: 100rpx;
  height: 100rpx;
}
.bank_top{
  width: 540rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.bank_top>view>view:nth-child(1){
  font-size: 34rpx;
}
.bank_top>view>view:nth-child(2){
  font-size: 30rpx;
  color: rgb(167, 164, 164);
  margin-top: 30rpx;
}
.bank_top>image{
  width: 40rpx;
  height: 40rpx;
}
.bank_botton{
 height: 90rpx;
 display: flex;
 padding: 0 40rpx;
 align-items: center;
 background: white;
 border-top: 1rpx solid #eeeeee;
}
.bank_botton>text:nth-child(1){
  font-size: 60rpx;
  color: #999999;
  margin-right: 20rpx;
}
.bank_botton>text:nth-child(2){
  font-size: 36rpx;
  position: relative;
  top: -1rpx;
}

/* <!-- 《无数据状态 --> */
.teamAward_all{
  width: 100%;
  height: 90%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.teamAward_imgt{
  margin-top: 200rpx;
}
.teamAward_imgt>image{
  width: 320rpx;
  height: 300rpx;
  margin-left: 100rpx;
}
.teamAward_imgt>view{
  margin: 20rpx 60rpx 80rpx;
  font-size: 30rpx;
  text-align: center;
}
.add_bank{
  width: 400rpx;
  height: 90rpx;
  border: 1rpx solid #f30505;
  color: #f30505;
  border-radius: 60rpx;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}