/* pages/people_list/people_list.wxss */
.people_list{
 padding: 30rpx;
 border-top: 1rpx solid #eeeeee;
 width: 100%;
 box-sizing: border-box;
 display: flex;
 align-items: center;
}
.people_list>image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.people>view:nth-child(1){
  font-size: 30rpx;
  margin: 10rpx 0 20rpx;
}
.people>view:nth-child(2){
  font-size: 30rpx;
  color: #999999
}