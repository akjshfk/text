/* pages/address/address.wxss */
/* addressList.css */
page{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.addressList{
  width: 100%;
  height: 100%;
  padding-top: 22rpx;
}
.addressList_box{
  width: 100%;
  margin-bottom: 22rpx;
  background-color: white;
}
.addressList_box_top{
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eeeeee;
}
.addressList_box_top_tops{
  display: flex;
  justify-content: space-between;
}
.addressList_box_top>text{
  font-size: 32rpx;
  position: relative;
  top: 16rpx;
}
.addressList_box_top_tops>text{
  font-size: 32rpx;
}
.addressList_box_botton{
  width: 100%;
  height: 90rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.addressList_box_botton_left>icon{
  position: relative;
  top: 8rpx;
}
.addressList_box_botton_left>text{
  margin-left: 20rpx;
  font-size: 30rpx;
}
.addressList_box_botton_right{
  display: flex;
 
}
.bl>image, .sc>image{
  width: 36rpx;
  height: 40rpx;
}
.bl>text, .sc>text{
  font-size: 30rpx;
  margin: 0 20rpx;
}
.nulls{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  color: #5c5a5a
}
/* 图片 */
.boxs{
  width: 100%;
  display: flex;
  justify-content: center;
  margin-top: 240rpx;
}
.login_box>image{
  width: 430rpx;
  height: 320rpx;
}
.message{
  margin-top: 30rpx;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #aaa7a7;
}


.add{
  padding:0 60rpx;
  margin-top: 200rpx
}
.add>view{
  display: flex;
  height: 100rpx;
  border-radius: 60rpx;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  background-color: #F8473D;
  color: white;
}
.add_site{
  width: 300rpx;
  height: 80rpx;
  font-size: 30rpx;
  color: #F8473D;
  border-radius: 50rpx;
  border: 1rpx solid #F8473D;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 40rpx 0 0 60rpx;
}
