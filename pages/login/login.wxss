/* pages/login/login.wxss */
page{
  background: white;
  padding: 220rpx 30rpx;
  display: flex;
  justify-content: center;
  box-sizing: border-box;
  width: 100%;
}
.login{
  width: 100%;
}
.login>view{
  display: flex;
  align-items: center;
  justify-content: center;
}
.imgt>image{
  width: 280rpx;
  height: 60rpx;
  margin-bottom: 160rpx;
}
.login_box{
  height: 90rpx;
  margin-bottom: 30rpx;
  border-bottom: 1rpx solid #eeeeee;
}
.login_box>image{
  width: 50rpx;
  height: 60rpx;
}
.login_box>input{
   height: 90rpx;
   width: 560rpx;
   margin-left: 40rpx;
}
.login>button{
  height: 100rpx;
  background: #FC424B;
  color: white;
  display: flex;
  border-radius: 60rpx;
  align-items: center;
  justify-content: center;
  margin-top: 100rpx
}