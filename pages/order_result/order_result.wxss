/* pages/order_result/order_result.wxss */
page{
  background: white;
}
.order_result{
  width: 100vw;
  height: 100vh;
  display: flex;
  justify-content: center;
  margin-top: 300rpx;
}
.order_result_box>image{
  width: 200rpx;
  height: 200rpx;
}
.order_result_box>view:nth-child(2){
  color: #F8473D;
  width: 206rpx;
  text-align: center;
  margin-top: 10rpx;
}
.order_result_box>view:nth-child(3){
  margin: 30rpx 0 0 0rpx;
  width: 206rpx;
  height: 76rpx;
  display: flex;
  border-radius: 8rpx;
  justify-content: center;
  align-items: center;
  border: 1rpx solid #999999;
}