/* pages/my_team/my_team.wxss */
page{
  background: #f5f5f5;
}
.my_team{
  width: 100%;
  border-top: 1rpx solid #eeeeee;
}
.picker>input{
  text-align: center;
  margin-top: 5rpx;
  font-size: 24rpx;
}
.Time{
  display: flex;
  justify-content: space-between;
  height: 130rpx;
  padding: 0 110rpx;
  background: white;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  box-sizing: border-box;
}
.Time>view:nth-child(1), .Time>view:nth-child(2){
 width: 210rpx;
 height: 60rpx;
 padding: 10rpx 30rpx;
 box-sizing: border-box;
 border: 1rpx solid #cecbcb;
 border-radius: 30rpx;
 display: flex;
 justify-content: center;
 align-items: center;
}
.Time>view:nth-child(2){
  text-align: right;
}
.on>view:nth-child(1){
  color: #F8473D;
}
.Team{
  width: 100%;
  height: 160rpx;
  background: white;
  margin-top: 152rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 100rpx;
  box-sizing: border-box;
}
.Team_box>view:nth-child(1){
 text-align: center;
}
.Team_es{
  margin-top: 22rpx;
  padding: 40rpx 30rpx;
  background: white;
  display: flex;
  justify-content: space-between;
}
.Team_es>image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.Team_es_box{
  width: 580rpx;
}
.Team_es_box_top{
  display: flex;
  justify-content: space-between;
}
.Team_es_box_botton{
  margin-top: 20rpx;
}
.Team_es_box_top>view:nth-child(2){
  color: #F8473D;
}

/*数据为空时 */
.boxs{
  width: 100%;
  display: flex;
  justify-content: center;
  margin-top: 240rpx;
}
.login_box>image{
  width: 430rpx;
  height: 320rpx;
}
.message{
  margin-top: 30rpx;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #aaa7a7;
}
